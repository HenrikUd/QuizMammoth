{
  "version": 2,
  "builds": [
    {
      "src": "MammothBackEnd/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "MammothFrontEnd/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "MammothFrontEnd/build/$1" }
  ]
}
