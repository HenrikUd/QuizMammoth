{
  "version": 2,
  "builds": [
    {
      "src": "MammothFrontEnd/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "MammothFrontEnd/build"
      }
    },
    {
      "src": "MammothBackEnd/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "MammothFrontEnd/build/$1"
    }
  ]
}
